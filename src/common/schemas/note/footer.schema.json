{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "https://ze.io/common/schemas/note/footer.schema.json",
  "title": "Note Footer",
  "description": "The footer of a note",
  "type": "object",
  "properties": {
    "tags": {
      "type": "array",
      "items": {
        "$ref": "https://ze.io/common/schemas/note/tag.schema.json"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "links": {
      "type": "array",
      "description": "The links associated with the note.",
      "items": {
        "description": "Links associated with the note.",
        "$ref": "./link.schema.json"
      },
      "minItems": 1,
      "uniqueItems": true
    }
  }
}
